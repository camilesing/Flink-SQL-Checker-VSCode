{
    "name": "flink-sql",
    "version": "1.4.0",
    "displayName": "Flink SQL",
    "description": "Adds support for Flink's variant of the SQL language",
    "publisher": "CamileSing",
    "author": {
        "name": "CamileSing",
        "email": "CamileSing@gmail.com"
    },
    "license": "MIT",
    "icon": "extras/apache_flink.png",
    "engines": {
        "vscode": "^1.20.0"
    },
    "galleryBanner": {
        "color": "#F2C45A",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/camilesing/flink-sql-vscode"
    },
    "homepage": "https://github.com/camilesing/flink-sql-vscode/master/README.md",
    "categories": [
        "Programming Languages",
        "Formatters"
    ],
    "keywords": [
        "Flink SQL",
        "Flink",
        "Formatter"
    ],
    "preview": false,
    "contributes": {
        "languages": [
            {
                "id": "flink-sql",
                "aliases": [
                    "FlinkSQL",
                    "flink-sql",
                    "Flink SQL",
                    "fql"
                ],
                "extensions": [
                    ".sql",
                    ".fql"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "flink-sql",
                "scopeName": "source.flink",
                "path": "./syntaxes/flink.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "flink-sql",
                "path": "./snippets/flink-sql.json"
            }
        ]
    },
    "activationEvents": [
        "onLanguage:fql",
        "onLanguage:flink-sql"
    ],
    "main": "./src/extension.js",
    "dependencies": {
        "sql-formatter-plus": "git+ssh://git@github.com:AChangFeng/sql-formatter-plus.git#v0.0.3"
    }
}